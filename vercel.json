{
  "version": 2,
  "builds": [
    {
      "src": "**/*.{html,css,js,svg,md}",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/zero-cost-skzlive",
      "dest": "/zero-cost-skzlive.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}